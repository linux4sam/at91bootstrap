config LINUX_KERNEL_ARG_STRING
	default "console=ttyS0,115200 mtdparts=atmel_nand:4M(bootstrap/kernel)ro,-(rootfs) rw rootfstype=ubifs ubi.mtd=1 root=ubi0:rootfs" if SAM9260_GLASNOST && !SDCARD
	default "mem=64M console=ttyS0,115200 root=/dev/mmcblk0p2 rw rootfstype=ext4 rootwait" if SAM9260_GLASNOST && SDCARD && RAM_64MB
	default "mem=128M console=ttyS0,115200 root=/dev/mmcblk0p2 rw rootfstype=ext4 rootwait" if SAM9260_GLASNOST && SDCARD && RAM_128MB


